(#
 (struct int_pair
  (int a)
  (int b))
 
 (var int_pair obj)
 (= (. a obj) (int 3))
 (= (. b obj) (int 4))
)

(function fib ((int n)) int
 (if (<= n (int 1))
  (return n)
  (return
   (+
    (call fib (- n (int 1)))
    (call fib (- n (int 2)))))))

(function main () int
 (call fib (int 3))
 (return 0))
